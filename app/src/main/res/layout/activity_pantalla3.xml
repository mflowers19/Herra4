<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Pantalla3">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="VOTOS ESCRUTADOS"
        android:textSize="24sp"
        android:textStyle="normal|bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.135" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="59dp"
        android:layout_height="57dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        app:srcCompat="@drawable/logo" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="70dp"
        android:layout_height="55dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.985"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        app:srcCompat="@drawable/logou" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="409dp"
        android:layout_height="530dp"
        android:translationY="60dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="148dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView13"
                    android:layout_width="116dp"
                    android:layout_height="133dp"
                    android:translationX="40dp"
                    app:srcCompat="@drawable/candidato" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="136dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:translationX="40dp">

                    <TextView
                        android:id="@+id/textView4"
                        android:layout_width="235dp"
                        android:layout_height="53dp"
                        android:text="Voto en Blanco"
                        android:textAlignment="center"
                        android:textSize="24sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/vb"
                        android:layout_width="235dp"
                        android:layout_height="74dp"
                        android:layout_weight="1"
                        android:textAlignment="center"
                        android:textSize="20sp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="148dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="116dp"
                    android:layout_height="133dp"
                    android:translationX="40dp"
                    app:srcCompat="@drawable/c1" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="136dp"
                    android:orientation="vertical"
                    android:translationX="40dp">

                    <TextView
                        android:id="@+id/textView1"
                        android:layout_width="235dp"
                        android:layout_height="53dp"
                        android:gravity="center"
                        android:text="Vivian Valenzuala"
                        android:textAlignment="center"
                        android:textSize="24sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/c1"
                        android:layout_width="235dp"
                        android:layout_height="74dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textSize="20sp" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="155dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView10"
                    android:layout_width="116dp"
                    android:layout_height="133dp"
                    android:translationX="40dp"
                    app:srcCompat="@drawable/c2" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:translationX="40dp">

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="235dp"
                        android:layout_height="53dp"
                        android:gravity="center"
                        android:text="Omar Aizupurua"
                        android:textAlignment="center"
                        android:textSize="24sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/c2"
                        android:layout_width="235dp"
                        android:layout_height="74dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textSize="20sp" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="116dp"
                    android:layout_height="133dp"
                    android:translationX="40dp"
                    app:srcCompat="@drawable/c3" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:translationX="40dp">

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="235dp"
                        android:layout_height="53dp"
                        android:gravity="center"
                        android:text="Matin Candanedo"
                        android:textAlignment="center"
                        android:textSize="24sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/c3"
                        android:layout_width="235dp"
                        android:layout_height="74dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textSize="20sp" />
                </LinearLayout>

            </LinearLayout>

            <Button
                android:id="@+id/button4"
                android:layout_width="match_parent"
                android:layout_height="64dp"
                android:text="Listo"
                android:textSize="20sp" />

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>